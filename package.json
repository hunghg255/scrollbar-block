{
  "name": "scrollbar-block",
  "version": "0.0.1",
  "description": "A tiny library to block the scrollbar of the body element",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "pnpm type-check && bunchee -m",
    "dev": "bunchee --watch",
    "dev:website": "turbo run dev --filter=website...",
    "format": "prettier --write ."
  },
  "keywords": [
    "react",
    "scrollbar",
    "block",
    "lock"
  ],
  "author": "hunghg255 <giahung197bg@gmail.com>",
  "license": "MIT",
  "homepage": "https://scrollbar-block.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/hunghg255/scrollbar-block.git"
  },
  "bugs": {
    "url": "https://github.com/hunghg255/scrollbar-block/issues"
  },
  "devDependencies": {
    "@types/node": "20.5.7",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.18",
    "bunchee": "^4.4.6",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turbo": "1.6",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "packageManager": "pnpm@6.32.11"
}
